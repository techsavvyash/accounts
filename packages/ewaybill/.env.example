# E-Way Bill API Configuration
# Get your credentials from https://ewaybillgst.gov.in

# Required: Your GSTIN (15 characters)
EWAYBILL_GSTIN=27AAPFU0939F1ZV

# Required: E-Way Bill Portal Username
EWAYBILL_USERNAME=your_username

# Required: E-Way Bill Portal Password
EWAYBILL_PASSWORD=your_password

# Required: Application Key from E-Way Bill Portal
EWAYBILL_APP_KEY=your_app_key

# Optional: Client ID for additional authentication
EWAYBILL_CLIENT_ID=

# Optional: Client Secret for additional authentication
EWAYBILL_CLIENT_SECRET=

# API Base URL (Production or Sandbox)
# Production: https://api.ewaybillgst.gov.in
# Sandbox: https://sandbox.ewaybillgst.gov.in
EWAYBILL_BASE_URL=https://api.ewaybillgst.gov.in

# API Version (default: v1.03)
EWAYBILL_API_VERSION=v1.03

# Request timeout in milliseconds (default: 30000)
EWAYBILL_TIMEOUT=30000

# Enable/disable request/response logging
EWAYBILL_DEBUG=false

# Cache settings
EWAYBILL_CACHE_ENABLED=true
EWAYBILL_CACHE_TTL=3600000

# Retry settings
EWAYBILL_MAX_RETRIES=3
EWAYBILL_RETRY_DELAY=1000
